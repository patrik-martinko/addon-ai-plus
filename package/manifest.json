{
    "manifest_version": 3,
    "name": "Bing AI Plus",
    "description": "",
    "version": "1.0.0",
    "content_scripts": [
        {
            "matches": [
                "https://*.bing.com/*"
            ],
            "js": [
                "script.js"
            ]
        }
    ],
    "icons": {
        "16": "icon-16.png",
        "48": "icon-48.png",
        "128": "icon-128.png"
    },
    "permissions": [
        "storage"
    ],
    "options_page": "options.html"
}